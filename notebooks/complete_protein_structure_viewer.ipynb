     "def find_timestep_files(base_dir='..'):\n",
     "    \"\"\"Find and sort PDB files from diffusion process\"\"\"\n",
     "    # Typical pattern for diffusion timestep files\n",
     "    patterns = [\n",
     "        os.path.join(base_dir, 'diffusion_process', 'structure_timestep_*.pdb'),\n",
     "        os.path.join(base_dir, 'enhanced_diffusion_vis', 'structure_timestep_*.pdb')\n",
     "    ]\n",
     "    \n",
     "    timestep_files = []\n",
     "    for pattern in patterns:\n",
     "        timestep_files.extend(glob.glob(pattern))\n",
     "    \n",
     "    # Extract timestep numbers for sorting\n",
     "    def get_timestep(filename):\n",
     "        try:\n",
     "            basename = os.path.basename(filename)\n",
     "            timestep_str = basename.split('timestep_')[1].split('.')[0]\n",
     "            return int(timestep_str)\n",
     "        except:\n",
     "            return 0\n",
     "    \n",
     "    # Sort files by timestep (descending)\n",
     "    timestep_files.sort(key=get_timestep, reverse=True)\n",
     "    \n",
     "    # Create mapping of timestep to file\n",
     "    timestep_to_file = {get_timestep(f): f for f in timestep_files}\n",
     "    \n",
     "    return timestep_files, timestep_to_file\n",
     "\n",
     "# Find timestep files\n",
     "timestep_files, timestep_to_file = find_timestep_files()\n",
     "print(f\"Found {len(timestep_files)} files from diffusion process.\")\n",
     "\n",
     "if len(timestep_files) > 0:\n",
     "    # Extract timesteps\n",
     "    timesteps = sorted([int(os.path.basename(f).split('timestep_')[1].split('.')[0]) \n",
     "                         for f in timestep_files], reverse=True)\n",
     "    \n",
     "    # Create a slider to select timestep\n",
