     "                    elif color == 'secondary':\n",
     "                        viewer.setStyle({'cartoon': {}})\n",
     "                        viewer.setStyle({'helix': {'cartoon': {'color':'red'}}}) \n",
     "                        viewer.setStyle({'sheet': {'cartoon': {'color':'yellow'}}})\n",
     "                        viewer.setStyle({'loop': {'cartoon': {'color':'green'}}})\n",
