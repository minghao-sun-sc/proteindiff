     "    def on_timestep_change(change):\n",
     "        with diffusion_output:\n",
     "            diffusion_output.clear_output()\n",
     "            \n",
     "            selected_timestep = change['new']\n",
     "            selected_file = timestep_to_file[selected_timestep]\n",
     "            \n",
     "            # Calculate progress percentage\n",
     "            max_timestep = max(timesteps)\n",
