     "def get_multi_view_visualization(pdb_file):\n",
     "    \"\"\"Create multiple views of the same structure with different styles and coloring\"\"\"\n",
     "    # Read PDB file\n",
     "    with open(pdb_file) as f:\n",
     "        pdb_data = f.read()\n",
     "    \n",
     "    # Create a 2x2 grid of viewers\n",
     "    view = py3Dmol.view(viewergrid=(2,2), width=800, height=800, linked=True)\n",
     "    \n",
     "    # Add model to all viewers\n",
     "    for i in range(4):\n",
     "        view.addModel(pdb_data, 'pdb', {'viewer': i})\n",
     "    \n",
     "    # Viewer 0: Cartoon with rainbow coloring\n",
     "    view.setStyle({'cartoon': {'color': 'spectrum'}}, {'viewer': 0})\n",
     "    view.addLabel(\"Cartoon (Rainbow)\", {'position': {'x': 0, 'y': 0, 'z': -20}, \n",
     "                                       'backgroundColor': 'white', 'fontColor': 'black'}, \n",
     "                  {'viewer': 0})\n",
     "    \n",
     "    # Viewer 1: Surface representation\n",
     "    view.setStyle({'cartoon': {'color': 'white'}}, {'viewer': 1})\n",
     "    view.addSurface(py3Dmol.VDW, {'opacity': 0.7, 'colorscheme': 'whiteCarbon'}, {'viewer': 1})\n",
     "    view.addLabel(\"Surface\", {'position': {'x': 0, 'y': 0, 'z': -20}, \n",
     "                             'backgroundColor': 'white', 'fontColor': 'black'}, \n",
     "                  {'viewer': 1})\n",
     "    \n",
     "    # Viewer 2: Stick representation\n",
     "    view.setStyle({'stick': {}}, {'viewer': 2})\n",
     "    view.addLabel(\"Stick Model\", {'position': {'x': 0, 'y': 0, 'z': -20}, \n",
     "                                 'backgroundColor': 'white', 'fontColor': 'black'}, \n",
     "                  {'viewer': 2})\n",
     "    \n",
     "    # Viewer 3: Secondary structure coloring\n",
     "    view.setStyle({'cartoon': {}}, {'viewer': 3})\n",
     "    view.setStyle({'helix': {'cartoon': {'color':'red'}}}, {'viewer': 3})\n",
     "    view.setStyle({'sheet': {'cartoon': {'color':'yellow'}}}, {'viewer': 3})\n",
     "    view.setStyle({'loop': {'cartoon': {'color':'green'}}}, {'viewer': 3})\n",
     "    view.addLabel(\"Secondary Structure\", {'position': {'x': 0, 'y': 0, 'z': -20}, \n",
     "                                         'backgroundColor': 'white', 'fontColor': 'black'}, \n",
     "                  {'viewer': 3})\n",
     "    \n",
     "    # Zoom to fit in all viewers\n",
     "    view.zoomTo()\n",
     "    \n",
     "    return view\n",
     "\n",
     "if len(pdb_files) > 0:\n",
     "    # Create dropdown to select file for multi-view visualization\n",
     "    multi_view_dropdown = widgets.Dropdown(\n",
     "        options=[(os.path.basename(f), f) for f in pdb_files],\n",
     "        description='Structure:',\n",
     "        style={'description_width': 'initial'},\n",
     "        layout=widgets.Layout(width='50%')\n",
     "    )\n",
     "    \n",
     "    # Create button for multi-view visualization\n",
     "    multi_view_button = widgets.Button(\n",
     "        description='Show Multi-View',\n",
     "        button_style='primary',\n",
     "        tooltip='Show multiple views of the structure'\n",
     "    )\n",
     "    \n",
     "    # Create output widget\n",
     "    multi_view_output = widgets.Output()\n",
     "    \n",
     "    # Function to create multi-view visualization\n",
     "    def on_multi_view_button_click(b):\n",
     "        with multi_view_output:\n",
     "            multi_view_output.clear_output()\n",
     "            selected_file = multi_view_dropdown.value\n",
     "            \n",
     "            print(f\"Creating multi-view visualization for {os.path.basename(selected_file)}...\")\n",
     "            view = get_multi_view_visualization(selected_file)\n",
     "            display(view.show())\n",
     "    \n",
     "    # Connect button click to function\n",
     "    multi_view_button.on_click(on_multi_view_button_click)\n",
     "    \n",
     "    # Display widgets\n",
     "    display(widgets.VBox([\n",
     "        widgets.Label(value=\"Create a multi-view visualization of a structure:\"),\n",
     "        widgets.HBox([multi_view_dropdown, multi_view_button])\n",
     "    ]))\n",
     "    display(multi_view_output)\n",
     "else:\n",
     "    print(\"Please upload PDB files first.\")"
    ]
