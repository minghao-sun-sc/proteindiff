     "    )\n",
     "    \n",
     "    color_dropdown = widgets.Dropdown(\n",
     "        options=['spectrum', 'chain', 'residue', 'element', 'secondary'],\n",
     "        value='spectrum',\n",
     "        description='Color:',\n",
     "        style={'description_width': 'initial'}\n",
     "    )\n",
     "    \n",
     "    surface_checkbox = widgets.Checkbox(\n",
     "        value=False,\n",
     "        description='Show Surface',\n",
     "        style={'description_width': 'initial'}\n",
